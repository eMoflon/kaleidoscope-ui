<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

 <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.kaleidoscope.ui.deltaeditor.DeltaEditor"
            contributorClass="org.eclipse.emf.ecore.presentation.EcoreActionBarContributor"
            default="true"
            extensions="xmi"
            icon="icons/deltaSmall-refit.png"
            id="com.kaleidoscope.ui.deltaeditor.DeltaEditor"
            name="Kaleidoscope Delta Editor">
   </editor>
   </extension>
 <extension
       point="net.sourceforge.plantuml.eclipse.diagramTextProvider">
    <diagramTextProvider
          fileExtensions="xmi"
          providerClass="com.kaleidoscope.ui.deltavisualisation.DeltaDiagrammTextProvider">
    </diagramTextProvider>
 </extension>
   
   
</plugin>
